<filters>
	<filter name="deny-interaction">
		<deny>
			<block>chest</block>
			<block>trapped chest</block>
		</deny>
	</filter>
    <filter name="wool-room" parents="deny-all">
        <allow><block>iron fence</block></allow>
        <allow><block>iron block</block></allow>
        <allow><block>diamond block</block></allow>
        <allow><block>thin glass</block></allow>
    </filter>
    <filter name="woolroom-b">
        <deny><team>blue</team></deny>
    </filter>
    <filter name="woolroom-r">
        <deny><team>red</team></deny>
    </filter>
    <filter name="spawn-b">
        <deny><team>blue</team></deny>
    </filter>
    <filter name="spawn-r">
        <deny><team>red</team></deny>
    </filter>
    <filter name="only-red">
        <team>red</team>
    </filter>
    <filter name="only-blue">
        <team>blue</team>
    </filter>
</filters>
<regions>
    <rectangle name="woolroom-r" min="52,-199" max="-54,-217"/>
    <rectangle name="woolroom-b" min="-54,50" max="52,67"/>
    <rectangle name="spawn-b" min="7,14" max="-8,46"/>
    <rectangle name="spawn-r" min="7,-163" max="-8,-196"/>
    <cuboid name="spawn-o" min="-34,50,-69" max="-44,38,-79"/>
    <rectangle name="yellow-woolroom" min="51,-216" max="34,-199"/>
    <rectangle name="orange-woolroom" min="-37,-216" max="-54,-199"/>
    <rectangle name="purple-woolroom" min="-36,50" max="-54,67"/>
    <rectangle name="blue-woolroom" min="52,50" max="34,67"/>
    <rectangle name="main" min="oo,oo" max="-oo,-oo"/>

    <apply kit="red-defender">
        <cuboid min="1,15,-183" max="3,16,-184"/>
    </apply>
    <apply kit="red-attacker">
        <cuboid min="-4,16,-183" max="-2,15,-184"/>
    </apply>
    <apply kit="blue-defender">
        <cuboid min="-4,16,34" max="-2,15,35"/>
    </apply>
    <apply kit="blue-attacker">
        <cuboid min="1,16,34" max="3,15,35"/>
    </apply>
    <apply block-break="deny-all" message="Do not break spawn!">
        <region name="spawn-o"/>
    </apply>
    <apply enter="woolroom-b" message="You may not enter your own wool room.">
        <region name="purple-woolroom"/>
        <region name="blue-woolroom"/>
    </apply>
    <apply enter="spawn-b" message="You may not enter the enemies spawn">
        <region name="spawn-r"/>
    </apply>
    <apply enter="spawn-r" message="You may not enter the enemies spawn">
        <region name="spawn-b"/>
    </apply>
    <apply enter="woolroom-r" message="You may not enter your own wool room.">
        <region name="yellow-woolroom"/>
        <region name="orange-woolroom"/>
    </apply>
    <apply block="deny-all" message="You may not edit spawn!">
        <region name="spawn-b"/>
        <region name="spawn-r"/>
    </apply>
    <apply block-place="deny-all" block-break="wool-room" message="You may only break iron and diamond blocks in this area.">
        <region name="woolroom-r"/>
        <region name="woolroom-b"/>
    </apply>
	<apply use="deny-interaction" message="`c`oStorage has been temporarily disabled to prevent class mixing.">
		<region name="main"/>
	</apply>
</regions>
