<?xml version="1.0"?>
<map proto="1.3.2">
<name>OCGP: Rage Valley</name>
<version>1.1.3</version>
<objective>Diamond Cup: Be the first car to finish 3 laps.</objective>
<include src="../race.xml"/>
<authors>
    <author>TheZaner</author>
</authors>
<rules>
	<rule>Accessing the mountains gains no advantage, do not attempt to go in them.</rule>
    <rule>Fall damage is disabled!</rule>
</rules>
<score>
    <limit>3</limit>
    <box value="1">
        <cuboid min="39,4,47" max="51,6,59"/>
    </box>
</score>
<destroyables name="Monument" materials="obsidian" completion="100%" owner="blue" mode-changes="true">
    <destroyable>
       <cuboid min="109,30,-4" max="109,31,6"/>
    </destroyable>
</destroyables>
<modes>
    <mode after="2s" material="redstone block" name="`a3"/>
    <mode after="3s" material="gold block" name="`e2"/>
    <mode after="4s" material="emerald block" name="`c1"/>
    <mode after="5s" material="air" name="`6Go!"/>
</modes>
<timelock>on</timelock>
<regions>
	<cylinder name="observers" base="0,49,-1" radius="2" height="0"/>
	<cuboid name="spawn" min="110,29,6" max="120,29,-4"/>
	<cuboid name="kit" min="39,4,47" max="51,6,59"/>
	<apply kit="kit">
		<region name="kit"/>
		<cuboid min="oo,1,oo" max="-oo,3,-oo"/>
		<cuboid min="110,35,7" max="111,25,-5"/>
	</apply>
